# LLM API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
PERPLEXITY_API_KEY=pplx-your-api-key-here

# Data APIs
NEWS_API_KEY=your_newsapi_key

# Database (SQLite - no configuration needed)
# Database file will be created at: storage/trading.db

# Cache (diskcache - file-based)
# Cache directory: storage/cache/

# Storage
S3_BUCKET=agentic-trading-docs
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key

# Trading Configuration
INITIAL_CASH=100000
MAX_POSITION_SIZE=0.05
MAX_CONCURRENT_POSITIONS=10

# Backtest Validation
BACKTEST_MIN_WIN_RATE=70.0
BACKTEST_CACHE_TTL_DAYS=90
BACKTEST_HISTORICAL_YEARS=5

# Monitoring
SENTRY_DSN=https://your_sentry_dsn
LOG_LEVEL=INFO

# Development
DEBUG=True
ENVIRONMENT=development
